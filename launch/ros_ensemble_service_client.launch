<launch>
	<arg name="server_name" default="ensemble" />
	<arg name="cmd" default="ensemble:run" />
	<arg name="marker_id" default="-1" />
	<arg name="orange_mx" default="0" />
	<arg name="orange_my" default="0" />
	<arg name="orange_mz" default="0" />
	<arg name="debug" default="1" />
	
	<node pkg="ros_ensemble_service" type="ros_ensemble_service_node_client" name="ensemble_service_node_client" >
		<!-- command -->
		<param name="cmd" type="string" value="$(arg cmd)" />
		
		<param name="marker_id" value="$(arg marker_id)" />
		<param name="orange_mx" value="$(arg orange_mx)" />
		<param name="orange_my" value="$(arg orange_my)" />
		<param name="orange_mz" value="$(arg orange_mz)" />

		<param name="debug" value="$(arg debug)" />
		
		<param name="server_name" value="$(arg server_name)" />
	</node>
</launch>
